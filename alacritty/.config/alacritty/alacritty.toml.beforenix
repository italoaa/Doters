live_config_reload = true
working_directory = "/Users/italo/"

[colors]
draw_bold_text_with_bright_colors = true

[colors.bright]
black = "#252931"
blue = "#519ABA"
cyan = "#88C0D0"
green = "#7eca9c"
magenta = "#B48EAD"
red = "#DE5852"
white = "#ECEFF4"
yellow = "#ebcb8b"

[colors.normal]
black = "#434C5E"
blue = "#81A1C1"
cyan = "#88C0D0"
green = "#A3BE8C"
magenta = "#B48EAD"
red = "#D08770"
white = "#ECEFF4"
yellow = "#EBCB8B"

[colors.primary]
background = "#2E3440"
foreground = "#ECEFF4"

[cursor]
unfocused_hollow = true
vi_mode_style = "None"

[cursor.style]
blinking = "Off"
shape = "Block"

[env]
TERM = "xterm-256color"

[font]
size = 16.0

[font.bold]
family = "FiraCode Nerd Font"
style = "Bold"

[font.italic]
family = "FiraCode Nerd Font"
style = "Italic"

[font.normal]
family = "FiraCode Nerd Font"
style = "Regular"

